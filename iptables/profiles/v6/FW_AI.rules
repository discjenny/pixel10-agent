-A FW_AI -m state --state ESTABLISHED,RELATED -j ACCEPT
-A FW_AI -o lo -j ACCEPT
# DHCPv6 client (546 -> 547)
-A FW_AI -p udp --sport 546 --dport 547 -j ACCEPT
# DNS
-A FW_AI -p udp --dport 53 -j ACCEPT
-A FW_AI -p tcp --dport 53 -j ACCEPT
# ICMPv6 essentials (RS 133, NS 135, Echo 128)
-A FW_AI -p ipv6-icmp --icmpv6-type 133 -j ACCEPT
-A FW_AI -p ipv6-icmp --icmpv6-type 135 -j ACCEPT
-A FW_AI -p ipv6-icmp --icmpv6-type 128 -j ACCEPT
# -A FW_AI -m owner --uid-owner 10123 -j ACCEPT   # Termux only
